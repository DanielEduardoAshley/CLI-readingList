{
    "name": "outside-cli",
    "version": "1.0.0",
    "license": "MIT",
    "bin": {
        "readingList": "bin/readingList"
    },
    "scripts": {
        "test": "$(npm bin)/jest --coverage && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
        "write-coveralls": "rm -rf .coveralls.yml && echo \"repo_token:$COVERAGE_TOKEN\" >> .coveralls.yml",
        "start": "node index.js"
    },
    "devDependencies": {
        "coveralls": "^3.0.9",
        "jest": "^24.9.0",
        "mock-fs": "^4.10.4"
    },
    "dependencies": {
        "axios": "^0.19.0",
        "cli-select": "^1.1.1",
        "minimist": "^1.2.2",
        "mz": "^2.7.0",
        "ora": "^4.0.3"
    },
    "jest": {
        "collectCoverage": true,
        "coverageReporters": [
            "json",
            "lcov",
            "text",
            "clover",
            "html"
        ],
        "collectCoverageFrom": [
            "**/*.{js,jsx}",
            "!**/node_modules/**",
            "!**/vendor/**",
            "!**/__fixtures__/**",
            "!**/coverage/**",
            "!index.js"
        ]
    }
}
